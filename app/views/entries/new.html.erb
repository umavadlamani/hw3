<h1>New Entry for</h1>

<%= form_with model: @entry do |form| %>
  <p>
    <%= form.label "title" %>
    <%= form.text_field "title" %>
  </p>  
  <p>
    <%= form.label "description" %>
    <%= form.text_field "description" %>
  </p>
  <p>
    <%= form.label "posted_on" %>
    <%= form.text_field "posted_on" %>
  </p>
  <p>
    <%= form.label "place_id" %>
    <%= form.text_field "place_id" %>
  </p>
  <%= form.submit %>
<% end %>